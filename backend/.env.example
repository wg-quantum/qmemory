# ===========================================
# QMemory Backend - 量子記憶再構成システム
# ===========================================

# ----- 量子コンピューティング設定 -----
# IBM Quantum Platform設定
IBM_QUANTUM_TOKEN=your_ibm_quantum_token_here
USE_REAL_QUANTUM=false

# 量子計算設定
QUANTUM_QUBIT_COUNT=8
QUANTUM_SHOTS=512
QUANTUM_CACHE_ENABLED=true
QUANTUM_CACHE_TTL=1800

# ----- AI/ML設定 -----
# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash-latest

# AI埋め込み設定
AI_EMBEDDING_DIMENSION=256
AI_CACHE_ENABLED=true
AI_CACHE_TTL=3600

# ----- 地理情報サービス設定 -----
# Nominatim API設定
GEOCODING_USER_AGENT=QMemory/2.0
GEOCODING_RATE_LIMIT_MS=1000
GEOCODING_CACHE_TTL=3600
GEOCODING_TIMEOUT=10

# ----- サーバー設定 -----
HOST=127.0.0.1
PORT=8010
DEBUG=false
LOG_LEVEL=INFO

# ----- CORS設定 -----
CORS_ORIGINS=["http://localhost:3000", "http://localhost:3001"]
CORS_CREDENTIALS=true

# ----- セキュリティ設定 -----
SECRET_KEY=your_secret_key_here_change_in_production
JWT_SECRET_KEY=your_jwt_secret_here_change_in_production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# ----- パフォーマンス設定 -----
# ワーカープロセス数
WORKERS=1

# メモリ使用量制限
MAX_MEMORY_DIMENSION=256
MAX_EMOTION_DIMENSION=64
MAX_POOL_SIZE=10

# 並列処理設定
MAX_CONCURRENT_THREADS=4

# ----- ログ設定 -----
LOG_FORMAT=json
LOG_FILE=logs/qmemory.log
LOG_ROTATION=daily
LOG_RETENTION_DAYS=30

# ----- 開発環境設定 -----
DEVELOPMENT_MODE=true
RELOAD_ON_CHANGE=true
ENABLE_PROFILING=false

# テスト設定
TESTING=false
TEST_DATABASE_URL=sqlite:///./test.db

# ----- エラー処理設定 -----
SENTRY_DSN=
ERROR_REPORTING_ENABLED=false

# ----- モニタリング設定 -----
METRICS_ENABLED=false
METRICS_PORT=8011
HEALTH_CHECK_ENABLED=true

# ----- 実験的機能 -----
EXPERIMENTAL_FEATURES_ENABLED=false
QUANTUM_ML_ENABLED=false
ADVANCED_CACHING_ENABLED=false